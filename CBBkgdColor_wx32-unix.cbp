<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CBBkgdColor wx3.2.x (Unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="./devel32/share/codeblocks/plugins/CBBkgdColor" prefix_auto="1" extension_auto="1" />
				<Option object_output="./.objs32/plugins/contrib/CBBkgdColor" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#cb)/devel32/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-std=gnu++20" />
					<Add option="-g" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.2" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="`$(WX_CONFIG) --cflags`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="$(#cb)/include" />
			<Add directory="$(#cb)/include/tinyxml" />
			<Add directory="$(#cb)/sdk/wxscintilla/include" />
			<Add directory="$(#cb)/src" />
		</Compiler>
		<Linker>
			<Add option="`$(WX_CONFIG) --libs`" />
			<Add option="-Wl,--no-undefined" />
			<Add option="-lwx_gtk3u_aui-3.2" />
			<Add library="codeblocks" />
			<Add directory="$(#cb)/devel32" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ./devel32/share/codeblocks/CBBkgdColor.zip manifest.xml" />
			<Add after="cp ./devel32/share/codeblocks/CBBkgdColor.zip    $(#cb)/devel32/share/codeblocks/CBBkgdColor.zip" />
			<Add after="cp ./devel32/share/codeblocks/plugins/libCBBkgdColor.so    $(#cb)/devel32/share/codeblocks/plugins/libCBBkgdColor.so" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="CBBkgdColor.cpp" />
		<Unit filename="CBBkgdColor.h" />
		<Unit filename="Makefile.am" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
